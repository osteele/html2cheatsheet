cheatsheet do
  title {{ title }}               # Will be displayed by Dash in the docset list
  docset_file_name {{ filename }}    # Used for the filename of the docset
  keyword {{ keyword }}            # Used as the initial search keyword (listed in Preferences > Docsets)

  {%- if resources_dir -%}
  resources '{{ resources_dir }}'
  {% endif %}

  {%- if introduction -%}
  introduction '{{ introduction }}'
  {% endif %}

  {% for category in categories %}
  category do
    id {{ category[0] }}

    {%- for entry in category[1] %}
    entry do
      name {{ entry.name }}
      command {{ entry.key }}
      notes {{ entry.notes }}
    end
    {% endfor %}
  end
  {% endfor %}
end
